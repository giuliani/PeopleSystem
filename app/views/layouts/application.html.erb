<!DOCTYPE html>
<html>
<head>
  <title>PeopleSystem</title>
  <%= stylesheet_link_tag href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" %>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js" %>
  <%= javascript_include_tag "https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js" %>
  <%= csrf_meta_tags %>

  <%= google_webfonts_init({
    google: ['Lobster Two', 'Oxygen']
  }) %>
</head>

<body>
  <div class="container">
    <%= yield %>
  </div>
</body>
</html>
